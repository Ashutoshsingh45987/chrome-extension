{
  "manifest_version": 3,
  "name": "Autofill Extension",
  "version": "1.0",
  "description": "Autofills input fields on web pages",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
    
  ],
  "action": {
    "default_popup": "popup.html" 
  },
  "content_scripts": [
    {
      "matches": ["*://*.google.com/*","*://*.github.com/*","*://*.example-extension.com/*"],  
      "js": ["content.js"]
    }
  ]
}